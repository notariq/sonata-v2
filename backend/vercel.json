{
    "version": 2,
    "builds": [
      {
        "src": "backend/api/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/dist/$1"
      }
    ]
  }
  